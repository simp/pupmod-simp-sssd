
# sssd::service::ssh
[ssh]
<% if @description -%>
description = <%= @description %>
<% end -%>
<% if @debug_level -%>
debug_level = <%= @debug_level %>
<% end -%>
debug_timestamps = <%= @debug_timestamps.to_s %>
debug_microseconds = <%= @debug_microseconds.to_s %>
ssh_hash_known_hosts = <%= @ssh_hash_known_hosts.to_s %>
<% if @ssh_known_hosts_timeout -%>
ssh_known_hosts_timeout = <%= @ssh_known_hosts_timeout.to_s %>
<% end -%>
