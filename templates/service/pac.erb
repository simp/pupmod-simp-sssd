
# sssd::service::pac
[pac]
<% if @description -%>
description = <%= @description %>
<% end -%>
<% if @debug_level -%>
debug_level = <%= @debug_level %>
<% end -%>
debug_timestamps = <%= @debug_timestamps.to_s %>
debug_microseconds = <%= @debug_microseconds.to_s %>
<% unless @allowed_uids.empty? -%>
allowed_uids = <%= Array(@allowed_uids).join(',') %>
<% end -%>
