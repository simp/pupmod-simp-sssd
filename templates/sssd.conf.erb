[sssd]
<% unless @debug_level.empty? -%>
debug_level = <%= @debug_level %>
<% end -%>
<% unless @debug_timestamps.empty? -%>
debug_timestamps = <%= @debug_timestamps %>
<% end -%>
<% unless @debug_microseconds.empty? -%>
debug_microseconds = <%= @debug_microseconds%>
<% end -%>
<% unless @description.empty? -%>
description = <%= @description %>
<% end -%>
domains = <%= Array(@domains).join(',') %>
config_file_version = <%= @config_file_version %>
services = <%= Array(@services).join(',') %>
reconnection_retries = <%= @reconnection_retries %>
<% unless @re_expression.empty? -%>
re_expression = <%= @re_expression %>
<% end -%>
<% unless @full_name_format.empty? -%>
full_name_format = <%= @full_name_format %>
<% end -%>
<% unless @try_inotify -%>
try_inotify = <%= @try_inotify %>
<% end -%>
<% unless @krb5_rcache_dir -%>
krb5_rcache_dir = <%= @krb5_rcache_dir %>
<% end -%>
<% unless @user -%>
user = <%= @user %>
<% end -%>
<% unless @default_domain_suffix -%>
default_domain_suffix = <%= @default_domain_suffix %>
<% end -%>
<% unless @override_space-%>
override_space = <%= @override_space%>
<% end -%>
