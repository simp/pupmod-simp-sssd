{
  "name": "simp-sssd",
  "version": "7.0.0",
  "author": "SIMP Team",
  "summary": "Manages SSSD",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-sssd",
  "project_page": "https://github.com/simp/pupmod-simp-sssd",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
